{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Consultation",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique identifier for the consultation"
    },
    "userId": {
      "type": "string",
      "description": "Identifier for the user requesting the consultation"
    },
    "date": {
      "type": "string",
      "format": "date-time",
      "description": "Date and time of the consultation"
    },
    "status": {
      "type": "string",
      "enum": ["scheduled", "completed", "canceled"],
      "description": "Current status of the consultation"
    },
    "notes": {
      "type": "string",
      "description": "Additional notes for the consultation"
    }
  },
  "required": ["id", "userId", "date", "status"],
  "additionalProperties": false
}